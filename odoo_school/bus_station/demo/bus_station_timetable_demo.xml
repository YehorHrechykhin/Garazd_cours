<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="bus_station_timetable_1" model="bus.station.timetable" forcecreate="True">
        <field name="route_id" ref="bus_station_route_kharkiv_kiev"/>
        <field name="driver_id" ref="base.res_partner_address_100"/>
        <field name="bus_id" ref="bus_station_bus_mercedes"/>
        <field name="departure_date" eval="datetime.now()"/>
        <field name="arrival_date" eval="datetime.now() + timedelta(hours=6)"/>
    </record>

    <record id="bus_station_timetable_2" model="bus.station.timetable" forcecreate="True">
